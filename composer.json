{
    "name": "webflorist/routetree",
    "description": "Advanded route management for Laravel 5",
    "authors": [
        {
            "name": "Gerald Buttinger",
            "email": "gerald@code.florist"
        }
    ],
    "require": {
        "laravel/framework": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Webflorist\\RouteTree\\": "src/",
	        "RouteTreeTests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webflorist\\RouteTree\\RouteTreeServiceProvider"
            ]
        }
    }
}
